<dl class="dl-horizontal attributes-catalogue">
	<% phone.attributes_for_display.each do |attr, value| %>
	<% label = attr.to_s == "camera" ? "Kamera" : I18n.t("mongoid.attributes.phone."+attr) %>
	<%= render :partial => "phone_attribute_small_item", :locals => {:label => label, :value => value} %>
	<% end %>
</dl>
