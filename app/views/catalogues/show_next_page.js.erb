$("#loading_big").remove();
<% @offers.each do |offer| %>
var offer = $("<%= escape_javascript(render(:partial => 'phones/offer_item', :locals => {:offer => offer})) %>");
$(".offers").append(offer);
<% end %>

<% if @to < @all_offers.length - 1 %>
var load_more = $("<%= escape_javascript(render(:partial => 'phones/load_more_offers')) %>");
$(".phone-right-sidebar").append(load_more);
<% end %>